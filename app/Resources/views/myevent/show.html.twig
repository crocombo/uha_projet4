{% extends 'base.html.twig' %}

{% block body %}
    <h1>Myevent</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ myEvent.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ myEvent.name }}</td>
            </tr>
            <tr>
                <th>Typeoftheevent</th>
                <td>{{ myEvent.typeOfTheEvent }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ myEvent.description }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('myevent_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('myevent_edit', { 'id': myEvent.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
